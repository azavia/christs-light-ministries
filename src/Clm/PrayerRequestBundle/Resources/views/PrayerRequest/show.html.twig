<h1>PrayerRequest</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Name</th>
            <td>{{ entity.name }}</td>
        </tr>
        <tr>
            <th>Email</th>
            <td>{{ entity.email }}</td>
        </tr>
        <tr>
            <th>Contact_method</th>
            <td>{{ entity.contactmethod }}</td>
        </tr>
        <tr>
            <th>Phone_number</th>
            <td>{{ entity.phonenumber }}</td>
        </tr>
        <tr>
            <th>Skype_username</th>
            <td>{{ entity.skypeusername }}</td>
        </tr>
        <tr>
            <th>Subject</th>
            <td>{{ entity.subject }}</td>
        </tr>
        <tr>
            <th>Prayer_request</th>
            <td>{{ entity.prayerrequest }}</td>
        </tr>
        <tr>
            <th>Ip_address</th>
            <td>{{ entity.ipaddress }}</td>
        </tr>
        <tr>
            <th>Private</th>
            <td>{{ entity.private }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('prayer-requests') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('prayer-requests_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('prayer-requests_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
